{
  "name": "clingo-wasm",
  "version": "0.0.1",
  "description": "Clingo compiled to WebAssembly",
  "source": "index.js",
  "repository": "https://github.com/domoritz/clingo-wasm",
  "author": "Dominik Moritz <domoritz@gmail.com>",
  "license": "Apache-2.0",
  "private": false,
  "type": "module",
  "scripts": {
    "build": "docker run -it -v $(pwd):/src trzeci/emscripten bash -c 'scripts/install_deps.sh; scripts/build.sh'",
    "copy": "cp clingo.js clingo.wasm examples/js",
    "deploy": "yarn copy && gh-pages -d examples",
    "start": "yarn copy && http-server examples"
  },
  "dependencies": {
    "@types/emscripten": "^1.38.0"
  },
  "devDependencies": {
    "gh-pages": "^2.1.1",
    "http-server": "^0.11.1"
  }
}
